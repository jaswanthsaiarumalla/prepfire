{
    "name": "prepfire-client",
    "version": "1.0.0",
    "description": "AI-Powered Coding Preparation Platform - Frontend",
    "private": true,
    "dependencies": {
      "@monaco-editor/react": "^4.6.0",
      "@testing-library/jest-dom": "^5.17.0",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^14.5.2",
      "axios": "^1.6.8",
      "gsap": "^3.12.5",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.22.3",
      "react-scripts": "5.0.1",
      "three": "^0.162.0",
      "@react-three/fiber": "^8.16.1",
      "@react-three/drei": "^9.105.4",
      "framer-motion": "^11.0.24",
      "lucide-react": "^0.368.0",
      "react-hot-toast": "^2.4.1",
      "react-syntax-highlighter": "^15.5.0",
      "react-helmet-async": "^2.0.4",
      "lodash": "^4.17.21",
      "classnames": "^2.5.1",
      "date-fns": "^3.6.0",
      "recharts": "^2.12.3",
      "react-intersection-observer": "^9.8.2",
      "web-vitals": "^2.1.4"
    },
    "devDependencies": {
      "tailwindcss": "^3.4.3",
      "autoprefixer": "^10.4.19",
      "postcss": "^8.4.38",
      "@types/lodash": "^4.17.0",
      "@types/three": "^0.162.0",
      "eslint": "^8.57.0",
      "eslint-plugin-react": "^7.34.1",
      "eslint-plugin-react-hooks": "^4.6.0",
      "prettier": "^3.2.5"
    },
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
      "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
      "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
      "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
      "dev": "npm start"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ],
      "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off"
      }
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    },
    "proxy": "http://localhost:5000",
    "engines": {
      "node": ">=16.0.0",
      "npm": ">=8.0.0"
    },
    "keywords": [
      "react",
      "coding",
      "interview",
      "preparation",
      "ai",
      "gsap",
      "3d",
      "glassmorphism",
      "algorithms",
      "data-structures"
    ],
    "author": "PrepFire Team",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/prepfire/prepfire-client.git"
    },
    "bugs": {
      "url": "https://github.com/prepfire/prepfire-client/issues"
    },
    "homepage": "https://prepfire.dev"
  }